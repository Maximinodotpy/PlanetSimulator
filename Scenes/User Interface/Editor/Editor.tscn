[sub_resource type="GDScript" id="GDScript_1m1dd"]
resource_name = "File Menu"
script/source = "extends PopupMenu


# Called when the node enters the scene tree for the first time.
func _ready():
	index_pressed.connect(callback)

func callback(id: int):
	var item_text = get_item_text(id)

	if item_text == 'Save':
		EditorSaves.save_file()
	if item_text == 'Exit':
		EditorSaves.currentSaveName = ''
		get_tree().change_scene_to_file(\"res://Scenes/User Interface/Main.tscn\")
"

[sub_resource type="GDScript" id="GDScript_m62k6"]
resource_name = "File Name Edit"
script/source = "extends LineEdit


# Called when the node enters the scene tree for the first time.
func _ready():
	text = EditorSaves.currentSaveName

	text_submitted.connect(text_suggested)


func text_suggested(new_text: String):
	if not new_text in EditorSaves.get_all_save_names() or EditorSaves.currentSaveName == new_text:
		print(new_text)

		## TODO Rename old SAave

		EditorSaves.currentSaveName = new_text
		release_focus()


"

